<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns="http://java.sun.com/xml/ns/javaee"
xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
	<servlet>
		<servlet-name>tutorialsServiceServlet</servlet-name>
		<servlet-class>com.github.infinityzero.gwt_sample.tutorials.server.TutorialsServiceImpl</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>tutorialsServiceServlet</servlet-name>
		<url-pattern>com.github.infinityzero.gwt_sample.tutorials.Tutorials/tutorialsService</url-pattern>
	</servlet-mapping>
	
	<welcome-file-list>
		<welcome-file>Tutorials.html</welcome-file>
	</welcome-file-list>
</web-app>
